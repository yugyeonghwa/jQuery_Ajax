<?xml version="1.0" encoding="UTF-8"?>
<!-- 
	xml : 확장 마크업 언어
	HTML는 w3c 에서 정의한 태그를 사용한다.
	xml는 사용자가 태그를 만들어서 사용
	DTD라는 xml 문서의 구조 및 해당 문서를 사용할 수 있는 
	적법한 요소 및 속성을 정의한 것
	
	xml는 루트 하나만 존재한다.
 -->
 <products>
 	<product name="제너시스" count="5" />
 	<product name="아반떼" count="3" />
 	<product name="그렌저" count="4" />
 	<product name="BMW" count="5" />
 	<product name="Volvo" count="2" />
		
 </products>